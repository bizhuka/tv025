<%@page language="abap" %>                                                                                                                                                                                                                                     
<%                                                                                                                                                                                                                                                             
  DATA(ls_result) = application->lock( ).                                                                                                                                                                                                                      
%>                                                                                                                                                                                                                                                             
{                                                                                                                                                                                                                                                              
  "message": "<%=ls_result-message%>",                                                                                                                                                                                                                         
  "error": <%=COND #( WHEN ls_result-error = abap_true                                                                                                                                                                                                         
                      THEN |true|                                                                                                                                                                                                                              
                      ELSE |false| )%>                                                                                                                                                                                                                         
}                                                                                                                                                                                                                                                              